{
    "version": "0.2",
    "language": "en-US,de-DE",
    "languageSettings": [
        {
            "languageId": "markdown",
            "ignoreRegExpList": [
                // pandoc references
                "\\[(@|\\^).+\\]"
            ]
        },
        {
            "languageId": "bibtex",
            "ignoreRegExpList": [
                // reference definition
                "@.+?\\{.+?,",
                "^.+?="
            ]
        }
    ],
    "ignorePaths": [
        "output-tex"
    ],
    "words": [
        "cadvisor",
        "chaosduck",
        "Crossplane",
        "DHBW",
        "failovers",
        "goroutines",
        "Istio",
        "knative",
        "kube",
        "kube-apiserver",
        "kube-prometheus",
        "Kubebuilder",
        "Kubeflow",
        "kubelet",
        "kubernetes",
        "Metacontroller",
        "pandoc",
        "Parca",
        "rebalancing",
        "resyncs",
        "schedulable",
        "sharded",
        "sharder",
        "strimzi",
        "subresource",
        "Velero",
        "webhosting",
        "Yaook"
    ]
}
